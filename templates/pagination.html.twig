<div class="bg-white dark:bg-gray-800">
<div class="mx-auto pb-7 max-w-7xl sm:px-6 lg:px-8">

{% if pages > 1 %}
<nav class="flex items-center justify-between px-4 sm:px-0">
  <div class="flex flex-1 w-0 -mt-px">
   {% if pages > 2 and page != 1 %}
    <a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge(app.request.query.all|merge({(get): page - 1}))) }}" {% if page == 1 %}disabled{% endif %} class="inline-flex items-center pt-4 pr-1 text-sm font-medium text-gray-500 border-t-2 border-transparent dark:text-gray-50 hover:text-gray-700 hover:border-gray-300">
      <!-- Heroicon name: solid/arrow-narrow-left -->
       <span class="mr-3 text-xl iconify" data-icon="line-md:arrow-left"></span>
      PrÃ©cÃ©dent
    </a>
     {% endif %}
  </div>
  <div class="hidden md:-mt-px md:flex">
  
    {% if page > 4 %}
        <a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge(app.request.query.all|merge({(get): 1}))) }}"  class="inline-flex items-center px-4 pt-4 text-sm font-medium text-gray-500 border-t-2 border-transparent dark:text-gray-50 hover:text-gray-700 hover:border-gray-300">
                   1
                </a>
               
            <span class="inline-flex items-center px-4 pt-4 text-sm font-medium text-gray-500 border-t-2 border-transparent dark:text-gray-50">
            ...
            </span>
            
    {% endif %}
        {% for p in range(max(page - 2, 1), min(page + 2, pages)) %}
            
            <a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge(app.request.query.all|merge({(get): p}))) }}" class="{% if p == page %}border-green-500 text-green-600{% else %}border-transparent text-gray-500 dark:text-gray-50 hover:text-gray-700 hover:border-gray-300{% endif %}  border-t-2 pt-4 px-4 inline-flex items-center text-sm font-medium">
                {{ p }}
            </a>
           
        {% endfor %}
         {% if page != pages %}
         {% if pages - page > 2 %}
            <span class="inline-flex items-center px-4 pt-4 text-sm font-medium text-gray-500 border-t-2 border-transparent dark:text-gray-50">
            ...
            </span>
            
                {% if pages > 2 %}
                
                    <a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge(app.request.query.all|merge({(get): pages}))) }}" class="inline-flex items-center px-4 pt-4 text-sm font-medium text-gray-500 border-t-2 border-transparent dark:text-gray-50 hover:text-gray-700 hover:border-gray-300">
                        {{pages}}
                    </a>
                
                {% endif %}
           {% endif %}
            {% endif %}
  </div>
  <div class="flex justify-end flex-1 w-0 -mt-px">
   {% if pages > 2 and page != pages %}
    <a href="{{ page < pages ? path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge(app.request.query.all|merge({(get): page +1 }))) : '#' }}" class="inline-flex items-center pt-4 pl-1 text-sm font-medium text-gray-500 border-t-2 border-transparent dark:text-gray-50 hover:text-gray-700 hover:border-gray-300">
      Suivant
      <!-- Heroicon name: solid/arrow-narrow-right -->
      <span class="ml-3 text-xl iconify" data-icon="line-md:arrow-right"></span>
    </a>
     {% endif %}
  </div>
</nav>

{% endif %}

</div>
</div>